CREATE TABLE stockvehicle(
	stockvehicle_id serial,
	stockvehicle_make varchar NOT NULL,
	stockvehicle_model varchar NOT NULL,
	stockvehicle_year smallint,
	stockvehicle_color varchar,
	stockvehicleisactive boolean,
	CONSTRAINT pk_stockvehicle_stockvehicle_id PRIMARY KEY (stockvehicle_id)
	);


